                                                <td>
                                                    <% if(solicitud.saldoFinal){ %>
                                                        <span style="color: green;">
                                                            <%= solicitud.saldoFinal %>
                                                        </span>
                                                        <% }else { %>
                                                            <span style="color: rgb(200, 130, 0);">Pendiente</span>
                                                            <% } %>
                                                </td>
                                                <td>
                                                    <%= solicitud.firmaContrato %>
                                                </td>
                                                <td class="text-danger">
                                                    <%= solicitud.incobrableMonto %>
                                                </td>
                                                <td>
                                                    <%= solicitud.ruta %>
                                                </td>
                                                <td>
                                                    <%= solicitud.celular %>
                                                </td>
                                                <td>
                                                    <% if (solicitud.clasificacionCliente == 'Sin Clasificacion') { %>
                                                        <p style="background-color: gray; color: white;">
                                                            <%= solicitud.clasificacionCliente %>
                                                        </p>
                                                        <% } else if (solicitud.clasificacionCliente == 'A-Premium') { %>
                                                            <p style="background-color: green; color: white;">
                                                                <%= solicitud.clasificacionCliente %>
                                                            </p>
                                                            <% } else if (solicitud.clasificacionCliente == 'B-Regular') { %>
                                                                <p style="background-color: yellow; color: black;">
                                                                    <%= solicitud.clasificacionCliente %>
                                                                </p>
                                                                <% } else if (solicitud.clasificacionCliente == 'C-Riesgo') { %>
                                                                    <p style="background-color: orange; color: black;">
                                                                        <%= solicitud.clasificacionCliente %>
                                                                    </p>
                                                                    <% } else if (solicitud.clasificacionCliente == 'D-Desestimado') { %>
                                                                        <p style="background-color: darkred; color: white;">
                                                                            <%= solicitud.clasificacionCliente %>
                                                                        </p>
                                                                        <% } %>
                                                </td>

                                                <td class="w-0 "><a id="eventoAct-Inact" href="/solicitudes-aprobadas/editar-solicitud/<%= solicitud.idSolicitud %>" class="btn " title="editar Solicitud"><i class="bi bi-pencil-square "></i></a>

                                                    <!-- <a id="eventoAct-Inact" href="/solicitudes-aprobadas/ver-solicitud/<%=  %>" class="btn " title="Ver Mensaje"><i class="bi bi-folder2-open "></i></a> -->
                                                    <!-- <a href="/rutas/eliminar-ruta/<%=  %>" type="submit" class=" btn me-lg-3"><i class="bi bi-x-lg me-lg-3" title="Eliminar Ruta"></i></a> -->
                                                </td>
                                            </tr>

                                            <% }) %>
                                                <% } %>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>


        </div>

    </div>


    </div>





