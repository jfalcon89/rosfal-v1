    <div class="container-fluid  mt-5 shadow-sm ">

        <div class="container-fluid ">

            <div class="accordion" id="accordionPanelsStayOpenExample">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="panelsStayOpen-headingOne">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseOne" aria-expanded="false" aria-controls="panelsStayOpen-collapseOne">
                            <h3 class="text-secondary"><strong>PRESTAMOS</strong> APROBADOS</h3>
                    </button>
                    </h2>
                    <div id="panelsStayOpen-collapseOne" class="accordion-collapse collapse " aria-labelledby="panelsStayOpen-headingOne">
                        <div class="accordion-body">
                            <div class="container-table table-responsive">
                                <table id="datatable" class="display responsive nowrap" style="width:100%">
                                    <thead class="thead-ligth">
                                        <tr>

                                            <th scope="col">ID Solicitud</th>
                                            <th scope="col">Nombre</th>
                                            <th scope="col">Apellido</th>
                                            <th scope="col">Cedula</th>
                                            <th scope="col">Fecha Solicitud</th>
                                            <th scope="col">Monto Aprobado</th>
                                            <th scope="col">Saldo Final</th>
                                            <th scope="col">Estado Solicitud</th>
                                            <th scope="col">Clasificacion</th>
                                            <th scope="col">Acciones</th>

                                        </tr>
                                    </thead>
                                    <tbody>

                                        <% if (arraySolicitudesAprobadas.length > 0) { %>
                                            <% arraySolicitudesAprobadas.forEach(solicitud=> { %>


                                                <tr>
                                                    <td>
                                                        <%= solicitud.idSolicitud %>
                                                    </td>
                                                    <td>
                                                        <%= solicitud.nombre %>
                                                    </td>
                                                    <td>
                                                        <%= solicitud.apellido %>
                                                    </td>
                                                    <td>
                                                        <%= solicitud.cedula %>
                                                    </td>
                                                    <td>
                                                        <%= solicitud.fechaSolicitud.toLocaleString('es-US', { timeZone: "America/Santo_Domingo" },) %>
                                                    </td>
                                                    <td>
                                                        <%= solicitud.montoSolicitado %>
                                                    </td>
                                                    <td>
                                                        <% if(solicitud.saldoFinal){ %>
                                                            <span style="color: green;">
                                                                <%= solicitud.saldoFinal %>
                                                            </span>
                                                            <% }else { %>
                                                                <span style="color: rgb(200, 130, 0);">Pendiente</span>
                                                                <% } %>
                                                    </td>
                                                    <td>
                                                        <%= solicitud.estadoSolicitud %>
                                                    </td>
                                                    <td>
                                                        <% if (solicitud.clasificacionCliente == 'Sin Clasificacion') { %>
                                                            <p style="background-color: gray; color: white;">
                                                                <%= solicitud.clasificacionCliente %>
                                                            </p>
                                                            <% } else if (solicitud.clasificacionCliente == 'A-Premium') { %>
                                                                <p style="background-color: green; color: white;">
                                                                    <%= solicitud.clasificacionCliente %>
                                                                </p>
                                                                <% } else if (solicitud.clasificacionCliente == 'B-Regular') { %>
                                                                    <p style="background-color: yellow; color: black;">
                                                                        <%= solicitud.clasificacionCliente %>
                                                                    </p>
                                                                    <% } else if (solicitud.clasificacionCliente == 'C-Riesgo') { %>
                                                                        <p style="background-color: orange; color: black;">
                                                                            <%= solicitud.clasificacionCliente %>
                                                                        </p>
                                                                        <% } else if (solicitud.clasificacionCliente == 'D-Desestimado') { %>
                                                                            <p style="background-color: darkred; color: white;">
                                                                                <%= solicitud.clasificacionCliente %>
                                                                            </p>
                                                                            <% } %>
                                                    </td>

                                                    <td class="w-0 "><a id="eventoAct-Inact" href="/solicitudes-aprobadas/editar-solicitud/<%= solicitud.idSolicitud %>" class="btn " title="editar Solicitud"><i class="bi bi-pencil-square "></i></a>
                                                        <!-- <a id="eventoAct-Inact" href="/solicitudes-aprobadas/ver-solicitud/<%=  %>" class="btn " title="Ver Mensaje"><i class="bi bi-folder2-open "></i></a> -->
                                                        <!-- <a href="/rutas/eliminar-ruta/<%=  %>" type="submit" class=" btn me-lg-3"><i class="bi bi-x-lg me-lg-3" title="Eliminar Ruta"></i></a> -->
                                                    </td>
                                                </tr>

                                                <% }) %>
                                                    <% } %>

                                    </tbody>
                                </table>



                            </div>
                        </div>
                    </div>
                </div>


            </div>


        </div>
