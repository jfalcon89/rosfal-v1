    <div class="mt-4 container-fluid">
        <div class="container-fluid d-xxl-flex justify-content-between" style="background-color: #ececec; ">

            <div class="flex-column" style="border-left: 7px solid #cbcbcb; padding-left: 16px">
                <h3 class="me-1 font-weight-bold text-truncate">Resumen</h3>

                <h6>
                    <%= porcentajeDeclinaciones %>% Declinadas <i class="bi bi-info-circle-fill" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Porcentaje de solicitudes declinadas vs todas las solicitudes recibidas"></i></h6>
                <h6>
                    <%= porcentajeEnRevision %>% En Revision <i class="bi bi-info-circle-fill" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Porcentaje de solicitudes en revision vs todas las solicitudes recibidas"></i></h6>
                <h6>
                    <%= porcentajeNuevas %>% Nuevas <i class="bi bi-info-circle-fill" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Porcentaje de solicitudes nuevas vs todas las solicitudes recibidas"></i></h6>
            </div>
            <div class="flex-column" style="border-left: 7px solid #cbcbcb; padding-left: 16px">
                <h3 class="me-1 font-weight-bold text-truncate">Prestamos Activo</h3>
                <h3 class="text-success">
                    <%= montoPrestado=Intl.NumberFormat('en-EN', { style: 'currency' , currency: 'USD'
                    }).format(montoPrestado) %>
                </h3>
                <h6>
                    <%= porcentajeAprobaciones %>% Aprobaciones <i class="bi bi-info-circle-fill" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Porcentaje de solicitudes aprobadas vs todas las solicitudes recibidas"></i></h6>
            </div>


            <div class="flex-column" style="border-left: 7px solid #cbcbcb; padding-left: 16px">
                <h3 class="me-1 font-weight-bold text-truncate">Liquidado</h3>
                <h3 class="" style="color: #e28724;">
                    <%= montoLiquidado=Intl.NumberFormat('en-EN', { style: 'currency' , currency: 'USD'
                    }).format(montoLiquidado) %>
                </h3>
                <h6>
                    <%= porcentajeLiquidado %>% Liquidados <i class="bi bi-info-circle-fill" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Porcentaje del total de prestamos saldados"></i></h6>
            </div>

            <div class="flex-column" style="border-left: 7px solid #cbcbcb; padding-left: 16px">
                <h3 class="me-1 font-weight-bold text-truncate">En Legal</h3>
                <h3 class="text-danger">
                    <%= prestamosEnLegal=Intl.NumberFormat('en-EN', { style: 'currency' , currency: 'USD'
                    }).format(prestamosEnLegal) %>
                </h3>
                <h6>
                    <%= porcentajeEnLegal %>% Monto En Legal <i class="bi bi-info-circle-fill" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Porcentaje de los prestamos que se encuentran en legal"></i></h6>
            </div>

            <div class="flex-column" style="border-left: 7px solid #cbcbcb; padding-left: 16px">
                <h3 class="me-1 font-weight-bold text-truncate ">Atrasos</h3>
                <h3 class="text-danger">
                    <%= prestamosAtrasos=Intl.NumberFormat('en-EN', { style: 'currency' , currency: 'USD'
                    }).format(prestamosAtrasos) %>
                </h3>
                <h6>
                    <%= porcentajeAtraso %>% Monto En Atraso <i class="bi bi-info-circle-fill" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Porcentaje de los prestamos que se encuentran en atrasos"></i></h6>
            </div>

            <div class="flex-column" style="border-left: 7px solid #cbcbcb; padding-left: 16px">
                <h3 class="me-1 font-weight-bold text-truncate ">Incobrables</h3>
                <h3 class="text-danger">
                    <%= prestamosIncobrables=Intl.NumberFormat('en-EN', { style: 'currency' , currency: 'USD'
                    }).format(prestamosIncobrables) %>
                </h3>
                <h6>
                    <%= porcentajeIncobrable %>% Incobrable <i class="bi bi-info-circle-fill" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Porcentaje de prestamos que no fueron pagados"></i></h6>
            </div>

            <div class="flex-column" style="border-left: 7px solid #cbcbcb; padding-left: 16px">
                <h3 class="me-1 font-weight-bold text-truncate">Cant. Activo</h3>
                <h3>
                    <%= cantPrestamos %>
                </h3>
            </div>

            <div class="flex-column" style="border-left: 7px solid #cbcbcb; padding-left: 16px">
                <h3 class="me-1 font-weight-bold text-truncate">Cant. Atrasos</h3>
                <h3>
                    <%= cantAtrasos %>
                </h3>
            </div>

        </div>
    </div>







