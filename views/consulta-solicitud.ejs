        <h5>Consulta de Estatus Solicitud de prestamo</h5>
    </div>
    <hr class="container">

    <div class="container mt-5 mb-5">
        <form action="" method="get">
            <div class="d-flex">
                <input class="form-control" type="text" placeholder="No. Cedula" name="id">
                <button class="btn btn-primary" type="submit">Buscar</button>
            </div>



        </form>

        <% if(id != "Vacio") { %>

            <div class="container mt-2">
                <table id="datatable" class="display responsive nowrap table table-sm-responsive table-hover" style="width:100%">
                    <thead class="thead-ligth">
                        <tr>
                            <th scope="col">Tipo de prestamo</th>
                            <th scope="col">Monto solicitud</th>
                            <th scope="col">Fecha solicitud</th>
                            <th scope="col">Estatus Solicitud</th>

                        </tr>
                    </thead>
                    <tbody>


                        <% if (arraySolicitud.length > 0) { %>
                            <% arraySolicitud.forEach(solicitud=> { %>

                                <tr>
                                    <td>
                                        <%= solicitud.tipoPrestamo %>
                                    </td>
                                    <td>
                                        <%= solicitud.montoSolicitado %>
                                    </td>
                                    <td>
                                        <%= solicitud.fechaSolicitud.toLocaleString('es-US', { timeZone: "America/Santo_Domingo"}).slice(0, 10) %>
                                    </td>
                                    <td>
                                        <%= solicitud.estadoSolicitud %>
                                    </td>
                                </tr>

                                <% }) %>
                                    <% } %>

                                        <% }else{ %>
                                            <h5 class="mt-5">Ningun Resultado</h5>
                                            <% } %>
                    </tbody>
                </table>
            </div>

    </div>




